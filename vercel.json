{
  "functions": {
    "src/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_GOOGLE_CLIENT_ID": "@google_client_id",
    "VITE_GOOGLE_CLIENT_SECRET": "@google_client_secret",
    "VITE_GOOGLE_API_KEY": "@google_api_key"
  }
}
