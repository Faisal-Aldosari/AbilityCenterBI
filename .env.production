# Production Environment Variables for AbilityCenterBI
# Copy these to your Vercel dashboard environment variables

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Google Services Configuration
VITE_GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key_here
VITE_BIGQUERY_PROJECT_ID=your_bigquery_project_id_here

# Gemini AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Application Configuration
VITE_APP_NAME=AbilityCenterBI
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# OAuth Redirect URLs (Update these with your production domain)
VITE_OAUTH_REDIRECT_URI=https://your-domain.vercel.app/auth/callback
VITE_OAUTH_ORIGIN=https://your-domain.vercel.app

# Analytics and Monitoring (Optional)
VITE_ANALYTICS_ID=your_analytics_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here
